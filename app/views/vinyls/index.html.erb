<p id="notice"><%= notice %></p>

<h1>Vinyls</h1>

<table>
  <thead>
    <tr>
      <th>Catalog</th>
      <th>Artist</th>
      <th>Title</th>
      <th>Label</th>
      <th>Format</th>
      <th>Rating</th>
      <th>Released</th>
      <th>Release</th>
      <th>Notes</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vinyls.each do |vinyl| %>
      <tr>
        <td><%= vinyl.catalog %></td>
        <td><%= vinyl.artist %></td>
        <td><%= vinyl.title %></td>
        <td><%= vinyl.label %></td>
        <td><%= vinyl.format %></td>
        <td><%= vinyl.rating %></td>
        <td><%= vinyl.released %></td>
        <td><%= vinyl.release_id %></td>
        <td><%= vinyl.notes %></td>
        <td><%= vinyl.date %></td>
        <td><%= link_to 'Show', vinyl %></td>
        <td><%= link_to 'Edit', edit_vinyl_path(vinyl) %></td>
        <td><%= link_to 'Destroy', vinyl, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vinyl', new_vinyl_path %>
